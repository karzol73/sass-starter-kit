{
  "name": "sass-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.pug",
    "build": "npm run clean && parcel build src/index.pug --public-url ./",
    "clean": "rimraf dist",
    "deploy": "gh-pages -d dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-purgecss": "^3.0.0",
    "postcss": "^8.2.8",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "> 1%",
          "last 4 versions",
          "ie >= 9"
        ]
      }
    }
  }
}
